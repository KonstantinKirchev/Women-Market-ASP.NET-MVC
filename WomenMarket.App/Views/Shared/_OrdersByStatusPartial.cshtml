@using WomenMarket.Models.ViewModels
@model IPagedList<MyOrderViewModel>

<table class="table table-striped table-hover ">
    <thead>
    <tr>
        <th>Order number</th>
        <th>Date of order</th>
        <th>Date of delivery</th>
        <th>Products</th>
        <th>Total amount</th>
        <th>Status</th>
    </tr>
    </thead>
    <tbody>
    @Html.DisplayFor(myOrder => myOrder)
    </tbody>
</table>

@if (Model.PageCount > 1)
{
    @Html.PagedListPager(Model, page => Url.Action("MyOrders", "Profile", new { page }))
}






